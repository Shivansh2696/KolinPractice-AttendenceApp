<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/nav_dashboard">
    <fragment
        android:id="@+id/nav_dashboard"
        android:name="com.example.kotlinpractice.Views.Fragments.DashboardFragment.DashBoardFragment"
        android:label="Dashboard"
        tools:layout="@layout/fragment_dash_board" />
    <fragment
        android:id="@+id/nav_settings"
        android:name="com.example.kotlinpractice.Views.Fragments.SettingsFragment.SettingFragment"
        android:label="Settings"
        tools:layout="@layout/fragment_setting" />

    <fragment
        android:id="@+id/nav_logout"
        android:name="com.example.kotlinpractice.Views.Fragments.LogoutFragment.LogoutFragment"
        android:label="Logout"/>
